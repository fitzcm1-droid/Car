public class SportsCar {
   private Car shield;
   private Car trunk;
   
   public void setShield(Car car) {
      shield = car;
   }
   
   public void setTrunk(Car car) {
      trunk = car;
   }
   
   public Car getShield() {
      return shield;
   }
   
   public Car getTrunk() {
      return trunk;
   }
   
   public void print() {
      shield.print();
      System.out.println();
      
      trunk.print();
      System.out.println();
   }
}
