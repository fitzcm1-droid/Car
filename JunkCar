public class JunkCar {
   public static void main(String[] args) {
      SportsCar park = new SportsCar();
      Car drive = new Car();
      Car reverse = new Car();
      
      drive.setGas("Forward");
      drive.setBrake("Right");
      park.setShield(drive);

      reverse.setGas("Backward");
      reverse.setBrake("Left");
      park.setTrunk(reverse);

      park.print();
   }
}
